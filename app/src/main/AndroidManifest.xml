<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ucf.knights.showlove.dudewheresmybook" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".BuySell"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Buy"
            android:label="@string/title_activity_buy"
            android:parentActivityName=".BuySell" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.ucf.knights.showlove.dudewheresmybook.BuySell" />
        </activity>
    </application>

</manifest>
